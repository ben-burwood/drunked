import data.drink.DrinkType;
import kotlin.Float;
import kotlin.Int;


CREATE TABLE Drink (
    id INTEGER AS Int PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL,
    abv REAL AS Float NOT NULL,
    type TEXT AS DrinkType NOT NULL
);

selectById:
SELECT * FROM Drink WHERE id = ?;

selectAll:
SELECT * FROM Drink;

insert:
INSERT INTO Drink(name, abv, type) VALUES (?, ?, ?);
